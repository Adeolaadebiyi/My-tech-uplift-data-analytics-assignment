def is_prime(num):
    if num <= 1:
        return False
    elif num <= 3:
        return True
    elif num % 2 == 0 or num % 3 == 0:
        return False
    i = 5
    while i * i <= num:
        if num % i == 0 or num % (i + 2) == 0:
            return False
        i += 6
    return True

# Number to check
num = 7

if is_prime(7):
    print(f"{7} is a prime number.")
else:
    print(f"{7} is not a prime number.")
